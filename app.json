{
  "name": "˹sifra ˼ [V2]",
  "description": "A Telegram Music Player Bot, written in Python with Pyrogram and Py-Tgcalls.",
  "logo": "https://telegra.ph/file/312bf3022b3f673cdf374.jpg",
  "keywords": [
    "python3",
    "telegram",
    "bot",
    "VenomX",
    "MusicBot",
    "telegram-bot",
    "pyrogram"
  ],
  "env": {
    "API_ID": {
      "description": "Get this value from https://my.telegram.org",
      "required": true
    },
    "API_HASH": {
      "description": "Get this value from https://my.telegram.org",
      "required": true
    },
    "BOT_TOKEN": {
      "description": "A Bot's token from Botfather",
      "required": true
    },
    "MONGO_DB_URI": {
      "description": "MongoDB URL from https://cloud.mongodb.com",
      "required": true
    },
    "OWNER_ID": {
      "description": "The user ID of the bot owner",
      "required": true
    },
    "STRING_SESSION": {
      "description": "Pyrogram v2 String Session (use @Shsusu_bot to generate)",
      "required": true
    },
    "LOGGER_ID": {
      "description": "Log Group ID (must be a group where the bot is admin)",
      "required": true
    },
    "HEROKU_API_KEY": {
      "description": "Optional: Your Heroku account's API key",
      "required": false
    },
    "HEROKU_APP_NAME": {
      "description": "Optional: Your Heroku app name",
      "required": false
    }
  },
  "stack": "container"
}
